{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "dom",
      "dom.iterable",
      "ES2021"
    ],
    "strict": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "strictBindCallApply": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "esModuleInterop": true,
      "declaration": true,
      "outDir": "../dist/public",
      "module": "ES6",
      "moduleResolution": "node",
      "noEmit": false,
      "noEmitOnError": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "jsx": "react-jsx",
      "baseUrl": "./"
  },
  "include": ["./src"]
}
